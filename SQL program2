package Sel;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

public class sqltask2 {

	public static void main(String[] args) throws SQLException {
		
		String database ="jdbc:mysql://localhost:3306/employee";
		Connection con = DriverManager.getConnection(database,"root","test");
		Statement sta = con.createStatement();
		
		sta.execute("Insert into db (empcode, empname, empage, esalary) values (101, 'Jenny', 25, 10000)");
		sta.execute("Insert into db (empcode, empname, empage, esalary) values (102, 'Jacky', 30, 20000)");
		sta.execute("insert into db (empcode, empname, empage, esalary) values (103, 'Joe', 20, 40000)");
		sta.execute("insert into db (empcode, empname, empage, esalary) values (104, 'John', 40, 80000)");
		sta.execute("insert into db (empcode,empname,empage,esalary) values (105,'Shameer', 25, 90000)");
		
		ResultSet rs = sta.executeQuery("select empcode, empname, empage, esalary from db");
		while(rs.next()) {
			System.out.println(rs.getString("empcode"));
			System.out.println(rs.getString("empname"));
			System.out.println(rs.getString("empage"));
			System.out.println(rs.getString("esalary"));
			
		}
	}

	
}
